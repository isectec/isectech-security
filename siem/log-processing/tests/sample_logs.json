  "validation_rules": {
    "required_fields": [
      "@timestamp",
      "event.action"
    ],
    "ip_address_fields": [
      "source.ip",
      "destination.ip",
      "client.ip"
    ],
    "port_fields": [
      "source.port",
      "destination.port"
    ],
    "severity_mapping": {
      "critical": 90,
      "high": 70,
      "medium": 50,
      "low": 30,
      "info": 10,
      "informational": 10
    },
    "outcome_mapping": {
      "success": "success",
      "allowed": "success",
      "pass": "success",
      "failure": "failure",
      "fail": "failure",
      "denied": "failure",
      "blocked": "failure",
      "error": "failure"
    }
  },
  "performance_benchmarks": {
    "minimum_throughput": {
      "logs_per_second": 1000,
      "description": "Minimum expected throughput for production deployment"
    },
    "maximum_latency": {
      "milliseconds": 100,
      "description": "Maximum acceptable latency per log entry"
    },
    "memory_usage": {
      "max_mb_per_1k_logs": 50,
      "description": "Maximum memory usage per 1000 log entries"
    }
  },
  "test_categories": {
    "functional": [
      "json_parsing",
      "syslog_parsing",
      "csv_parsing",
      "xml_parsing",
      "field_mapping",
      "data_type_conversion",
      "timestamp_parsing"
    ],
    "security": [
      "input_validation",
      "injection_prevention",
      "sensitive_data_masking",
      "access_control"
    ],
    "performance": [
      "batch_processing",
      "memory_efficiency",
      "concurrent_processing",
      "large_log_handling"
    ],
    "compliance": [
      "ecs_field_mapping",
      "required_field_validation",
      "data_retention",
      "audit_trail"
    ]
  }
