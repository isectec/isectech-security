{
  "name": "isectech-protect",
  "version": "1.0.0",
  "private": true,
  "description": "iSECTECH Protect - Enterprise Cybersecurity Command Center",
  "author": "iSECTECH",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:accessibility": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "test:lighthouse": "node scripts/lighthouse-test.js",
    "test:visual": "playwright test --grep '@visual'",
    "test:performance": "playwright test --grep '@performance'",
    "test:security": "node scripts/run-security-tests.js",
    "test:security:ci": "TEST_ENVIRONMENT=ci node scripts/run-security-tests.js",
    "test:security:staging": "TEST_ENVIRONMENT=staging node scripts/run-security-tests.js",
    "test:security:production": "TEST_ENVIRONMENT=production node scripts/run-security-tests.js --dry-run",
    "test:security:regression": "npm test -- __tests__/security/security-regression-automation.test.ts",
    "test:security:authorization": "npm test -- __tests__/security/comprehensive-api-authorization-matrix.test.ts",
    "test:security:baseline": "node scripts/run-security-tests.js --environment=ci --verbose",
    "test:all": "npm run test && npm run test:playwright && npm run test:accessibility",
    "test:ci": "npm run test:coverage && npm run test:playwright && npm run test:accessibility",
    "test:pact:consumer": "jest --testNamePattern=pact",
    "test:pact:provider": "npm run pact:verify",
    "test:load": "k6 run __tests__/performance/load-test.js",
    "test:load:artillery": "artillery run __tests__/performance/artillery-config.yml",
    "pact:publish": "pact-broker publish pacts --consumer-app-version=$npm_package_version",
    "pact:verify": "pact verify --provider-base-url=http://localhost:8080",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npm run type-check && npm run lint && npm run test",
    "analyze": "cross-env ANALYZE=true next build",
    "docs:validate": "ts-node docs/api/scripts/utils/doc-utils.ts validate",
    "docs:generate": "ts-node docs/api/scripts/generate-docs.ts",
    "docs:serve": "cd docs/api/generated && python3 -m http.server 8080",
    "docs:deploy": "ts-node docs/api/scripts/ci-cd-integration.ts",
    "docs:ci": "npm run docs:validate && npm run docs:generate && npm run docs:deploy",
    "docs:clean": "rm -rf docs/api/generated/*",
    "docs:watch": "nodemon --watch 'app/api/*.json' --watch 'backend/*.json' --ext json --exec 'npm run docs:generate'"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.4.5",
    "@mui/material": "^5.15.10",
    "@mui/icons-material": "^5.15.10",
    "@mui/x-charts": "^6.19.5",
    "@mui/x-data-grid": "^6.19.5",
    "@mui/x-date-pickers": "^6.19.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@emotion/cache": "^11.14.0",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-query-devtools": "^5.67.1",
    "zustand": "^4.5.2",
    "immer": "^9.0.21",
    "axios": "^1.7.9",
    "d3": "^7.9.0",
    "recharts": "^2.14.1",
    "framer-motion": "^12.1.0",
    "react-hook-form": "^7.55.1",
    "zod": "^3.25.1",
    "@hookform/resolvers": "^3.3.4",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "react-window": "^1.8.10",
    "react-virtualized-auto-sizer": "^1.0.24",
    "crypto-js": "^4.2.0",
    "uuid": "^11.0.4",
    "js-cookie": "^3.0.5",
    "react-helmet-async": "^2.0.5",
    "@next/bundle-analyzer": "^15.4.5",
    "sharp": "^0.33.5",
    "next-pwa": "^5.6.0",
    "workbox-webpack-plugin": "^7.1.0",
    "react-intersection-observer": "^9.13.2",
    "react-swipeable": "^7.0.1",
    "react-use-gesture": "^9.1.3",
    "react-spring": "^9.7.3",
    "swagger-ui-dist": "^5.10.5",
    "redoc": "^2.1.5"
  },
  "devDependencies": {
    "typescript": "^5.8.4",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@types/d3": "^7.4.3",
    "@types/lodash-es": "^4.17.12",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/react-window": "^1.8.8",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.14",
    "cypress": "^13.7.0",
    "@cypress/react18": "^2.0.1",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.4.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-security": "^3.0.1",
    "@axe-core/react": "^4.10.2",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "playwright": "^1.48.2",
    "@playwright/test": "^1.48.2",
    "lighthouse": "^12.2.1",
    "puppeteer": "^23.10.4",
    "pa11y": "^8.0.0",
    "pa11y-ci": "^3.1.0",
    "storybook": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "chromatic": "^11.21.0",
    "msw": "^2.6.8",
    "@storybook/addon-coverage": "^1.0.4",
    "jest-axe": "^9.0.0",
    "axe-playwright": "^2.0.3",
    "@pact-foundation/pact": "^13.1.1",
    "@pact-foundation/pact-node": "^10.17.7",
    "jest-pact": "^0.11.0",
    "k6": "^0.0.0",
    "artillery": "^2.0.11",
    "@redocly/cli": "^1.6.0",
    "@stoplight/spectral-cli": "^6.11.0",
    "swagger-parser": "^10.0.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
